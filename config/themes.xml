<?xml version="1.0" encoding="UTF-8"?>
<themes>
    <current>default</current>
    <theme name="default">
        <default>
            <foreground>#000000</foreground>
            <background>#FFFFFF</background>
        </default>
        <comments>
            <line>
                <foreground>#008000</foreground>
                <italic>true</italic>
            </line>
            <block>
                <foreground>#008000</foreground>
                <italic>true</italic>
            </block>
        </comments>
        <keywords>
            <control>
                <foreground>#0000FF</foreground>
                <bold>true</bold>
            </control>
            <types>
                <foreground>#2B91AF</foreground>
                <bold>true</bold>
            </types>
        </keywords>
        <strings>
            <foreground>#A31515</foreground>
        </strings>
        <numbers>
            <foreground>#FF7F00</foreground>
        </numbers>
        <operators>
            <foreground>#666666</foreground>
            <bold>true</bold>
        </operators>
        <constants>
            <foreground>#A349A4</foreground>
            <bold>true</bold>
        </constants>
        <functions>
            <foreground>#3F7F7F</foreground>
        </functions>
        <identifiers>
            <foreground>#000000</foreground>
        </identifiers>
        <linenumber>
            <foreground>#4B4B4B</foreground>
            <background>#F5F5F5</background>
        </linenumber>
        <currentLine>
            <background>#F0F0F0</background>
        </currentLine>
        <braceMatch>
            <foreground>#0000FF</foreground>
            <background>#FFFFFF</background>
            <bold>true</bold>
        </braceMatch>
        <braceMismatch>
            <foreground>#FF0000</foreground>
            <background>#FFFFFF</background>
            <bold>true</bold>
        </braceMismatch>
    </theme>
    <theme name="dark">
        <default>
            <foreground>#D4D4D4</foreground>
            <background>#1E1E1E</background>
        </default>
        <comments>
            <line>
                <foreground>#6A9955</foreground>
                <italic>true</italic>
            </line>
            <block>
                <foreground>#6A9955</foreground>
                <italic>true</italic>
            </block>
        </comments>
        <keywords>
            <control>
                <foreground>#C586C0</foreground>
                <bold>true</bold>
            </control>
            <types>
                <foreground>#4EC9B0</foreground>
                <bold>true</bold>
            </types>
        </keywords>
        <strings>
            <foreground>#CE9178</foreground>
        </strings>
        <numbers>
            <foreground>#B5CEA8</foreground>
        </numbers>
        <operators>
            <foreground>#D4D4D4</foreground>
            <bold>true</bold>
        </operators>
        <constants>
            <foreground>#4FC1FF</foreground>
            <bold>true</bold>
        </constants>
        <functions>
            <foreground>#DCDCAA</foreground>
        </functions>
        <identifiers>
            <foreground>#D4D4D4</foreground>
        </identifiers>
        <linenumber>
            <foreground>#858585</foreground>
            <background>#1E1E1E</background>
        </linenumber>
        <currentLine>
            <background>#282828</background>
        </currentLine>
        <braceMatch>
            <foreground>#C586C0</foreground>
            <background>#1E1E1E</background>
            <bold>true</bold>
        </braceMatch>
        <braceMismatch>
            <foreground>#F44747</foreground>
            <background>#1E1E1E</background>
            <bold>true</bold>
        </braceMismatch>
    </theme>
</themes>